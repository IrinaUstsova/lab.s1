#include <iostream>
#include <cmath>

using namespace std;

int main ()
{
    {
    cout << " ASCII: " << endl;
    char ch=0;
    for ( int i = 0 ; i <= 255 ; ++ i) 
      {
        cout << ch << " has ASCII code " << static_cast<int>(ch) << endl;
        ch ++;
      }
    }
    
    {
        cout << "Enter symbols" << endl;
    char a;
    cin >> a;
    if ((a >= '0') & (a <= '9')) cout << "digit" << endl;
    else if (((a >= 'A') && (a <= 'Z')) || ((a >= 'a') && (a <= 'z'))) cout << "letter" << endl;
    else cout << "symbols unknown" << endl;
    }

    {
        unsigned int x;
    cout << "Enter x" << endl;
    cin >> x;
    double y = 0;
    int z = x % 5;
    switch (z)
    {
        case 0: y = pow(x, 2); break;
        case 1: y = x; break;
        case 2: y = 0; break;
        case 3: y = -x; break;
        case 4: y = -1; break;
    }
    cout << y << endl;
    }
    
    {
        double e,sum=1.0;
    double y_1;
    int i=0; 
    cout << "Enter the accuracy" << endl;
    cin >> e;
    int x_4;
    cout << "Enter the x" << endl;
    cin >> x_4;
    y_1= cos (x_4);
    while((1 - y_1)>=e)
    {
        sum+=pow(-1,i) * pow(x_4, 2*i) / 2*i;
        i++;
    }
    cout.precision(7);
     cout << "For accuracy " << sum << " required " << i << " interations " << endl;
    }
    
    return 0;
}
