#include <iostream>
#include <cmath>

int main ()
{
    std :: cout << "Привет,";
    int x;
    std :: cout << "Введите x:";
    std :: cin >> x;
    int s = 3 * pow (x, 5) + 3 * pow (x, 3) -1;
    std :: cout << s << std :: endl;

    беззнаковый длинный int a;
    std :: cout << "Введите a:";
    std :: cin >> a;
    int t = 2 * abs (a) + sqrt (4 + 3 * a);
    std :: cout << t << std :: endl;

    bool k, l, m, n;
    std :: cout << "Введите k:";
    std :: cin >> k;
    std :: cout << "Введите l:";
    std :: cin >> l;
    std :: cout << "Введите m:";
    std :: cin >> m;
    std :: cout << "Введите n:";
    std :: cin >> n;
    std :: cout << (n || (m ^! k) &&! l) << std :: endl;

    int a;
    int b;
    std :: cout << "Введите b:";
    std :: cin >> b;
    а = pow (2, б);
    std :: cout << a << std :: endl;
    std :: cout << std :: oct << a << std :: endl;

    беззнаковый короткий int c;
    беззнаковый короткий int d;
    беззнаковый короткий int e;
    беззнаковый короткий int f;
    беззнаковый короткий int a;
    std :: cout << "Введите c:";
    std :: cin >> c;
    std :: cout << "Введите d:";
    std :: cin >> d;
    std :: cout << "Введите e:";
    std :: cin >> e;
    std :: cout << "Введите f:";
    std :: cin >> f;
    а = (! (c || d)) && (e ^ f);
    std :: cout << a << std :: endl;
    std :: cout << std :: hex << a << std :: endl;

    возврат 0;
}
